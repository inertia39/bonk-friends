{
	"manifest_version": 2,
	"name": "Bonk Friends",
	"version": "0.1.8",
	"author": "Inertia, (inertia_)",
	"description": "Notifies user when friend are on/offline.",
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"*://bonk.io/*"
	],
	"content_scripts": [
		{
			"matches": [
				"*://bonk.io/gameframe-release.html"
			],
			"js": [
				"userscript.js"
			],
			"all_frames": true
		}
	]
}
